{
  "name": "todolist",
  "version": "0.0.1",
  "description": "A ReactJS Todo List",
  "scripts": {
    "start": "node server.js",
    "lint": "eslint src",
    "test": "jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/lodash",
      "<rootDir>/node_modules/inspect-react-element",
      "<rootDir>/node_modules/indent",
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "components/generic"
    ],
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6",
      "jsx"
    ]
  },
  "author": "huanhulan <huanhulan@163.com> (http://github.com/huanhulan)",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^5.4.7",
    "babel-eslint": "^3.1.9",
    "babel-jest": "5.3.0",
    "babel-loader": "^5.1.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "classnames": "^2.1.3",
    "eslint": "^0.8.0",
    "eslint-plugin-react": "^2.3.0",
    "jest-cli": "0.7.1",
    "keymirror": "~0.1.0",
    "object-assign": "^1.0.0",
    "react-hot-loader": "~3.0.0-alpha.6",
    "react-test-renderer": "^15.4.1",
    "webpack": "^1.9.6",
    "webpack-dev-server": "^1.8.2"
  },
  "dependencies": {
    "camel-case": "^1.2.2",
    "classnames": "^2.2.3",
    "history": "^3.0.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
    "file-loader": "^0.8.4",
    "url-loader": "^0.5.6",
    "flux": "^2.1.1",
    "hammerjs": "^2.0.6"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}